{
  dataset_id: "decarb_2023_transport",
  dataset_type: "modeled",
  data_source: "tempo and evix",
  sector_description: "residential and commercial",
  description: "DECARB 6 results from TEMPO, EVI-X, and POLARIS (Nov 2023) with profiles using 2018 as the weather year",
  origin_creator: "Arthur Yip",
  origin_organization: "NREL",
  origin_contributors: [
    "Arthur Yip",
    "Brennan Borlaug"
  ],
  origin_project: "DECARB 6",
  origin_date: "2023-11-30",
  origin_version: "0.1.0",
  source: "TEMPO, EVI-X, and POLARIS runs for DECARB 6",
  data_classification: "low",
  tags: [
    "TEMPO",
    "EVIX",
    "POLARIS",
    "EV",
    "decarb",
  ],
  data_schema_type: "standard",
  trivial_dimensions: [
    "weather_year",
  ],
  use_project_geography_time_zone: true,
  data_schema: {
    load_data_column_dimension: "metric",
  },
  dimensions: [
    { 
     "class": "County",
      type: "geography",
      name: "US counties 2020 L48",
      display_name: "County",
      file: "dimensions/counties.csv",
      module: "dsgrid.dimension.standard",
      description: "US counties for L48, Census 2020 vintage, no territories",
    },
    {
      type: "sector",
      name: "Transportation",
      display_name: "Transportation",
      file: "dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      "class": "Sector",
      description: "TEMPO/EVI-X/POLARIS has modeled electric load associated with light, medium, and heavy-duty road transportation",
    },
    {
      type: "subsector",
      "class": "Subsector",
      description: "TEMPO/EVI-X/POLARIS specifies EV charging for specific EV market segments, classes, and technologies",
      file: "dimensions/subsectors.csv",
      module: "dsgrid.dimension.standard",
      name: "subsector",
      display_name: "EV market segment, class, and technology",
      
    },
    {
      type: "metric",
      name: "charging type",
      display_name: "Charging types",
      file: "dimensions/charging_types.csv",
      module: "dsgrid.dimension.standard",
      "class": "EnergyEndUse",
      description: "charging type, location and power level",
    },
    {
      type: "scenario",
      name: "DECARB High + EV Charge Management Strategy",
      display_name: "DECARB High + EV Charge Management Strategy",
      file: "dimensions/scenario.csv",
      module: "dsgrid.dimension.standard",
      "class": "Scenario",
      description: "DECARB High + EV Charge Management Strategy",
    },
    {
      type: "model_year",
      name: "model year",
      display_name: "model years",
      file: "dimensions/model_years.csv",
      module: "dsgrid.dimension.standard",
      "class": "ModelYear",     
      description: "model years, 2025 - 2050, every 5 years",
    },
    {
      "class": "WeatherYear",
      description: "Weather year, 2018 only",
      file: "dimensions/weather_year.csv",
      module: "dsgrid.dimension.standard",
      name: "2018 weather year",
      display_name: "2018 weather year",
      type: "weather_year",
    },
    {
      type: "time",
      class: "Time",
      frequency: 'P0DT1H0M0.000000S',
      name: "Time-2018-EST-hourly-periodBeginning-noLeapDayAdjustment-total",
      display_name: 'Time EST',
      time_type: 'datetime',
      leap_day_adjustment: 'none',
      description: 'Time dimension, 2018 hourly EST, hour-beginning, no Leap Day Adjustment, total value',
      time_interval_type: 'period_beginning',
      str_format: '%Y-%m-%d %H:%M:%S',
      timezone: 'UTC',
      measurement_type: 'total',
      ranges: [
            {
                start: '2018-01-01 00:00:00',
                end: '2018-12-31 23:00:00',
            },
        ],
    }, ]
}    
